{
  "name": "ice-utils",
  "version": "1.0.0",
  "description": "ZeroC ICE utilities",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
