  },
  "devDependencies": {
    "@types/tape": "^4.2.28",
    "rimraf": "^2.5.4",
    "tape": "^4.6.3",
    "ts-node": "^1.7.2",
    "tslint": "^3.15.1",
    "typed-ice": "^3.6.1",
    "typescript": "^2.1.4"
  }
}
